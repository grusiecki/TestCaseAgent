---
import Layout from '@/layouts/Layout.astro';
import { EditTitlesView } from '@/components/new-project/EditTitlesView';

// Get projectId from query parameters
const projectId = Astro.url.searchParams.get('projectId');

// Disable static generation for this page as it requires client-side data
export const prerender = false;
---

<Layout title="Edit Test Case Titles">
  <EditTitlesView client:load projectId={projectId || undefined} />
</Layout>
