# Test Environment Variables
# Copy this file to .env.test and fill in your test credentials
# DO NOT commit .env.test to version control

# Supabase Configuration (Test Instance)
SUPABASE_URL=https://your-test-project.supabase.co
SUPABASE_ANON_KEY=your-test-anon-key-here

# OpenRouter Configuration (Test)
OPENROUTER_API_KEY=your-test-openrouter-api-key
OPENROUTER_MODEL=openrouter/auto

# Application Configuration
JWT_MAX_AGE_DAYS=7
MAX_INPUT_CHARS=5000
MAX_TITLES_PER_SESSION=20

# Playwright Configuration
PLAYWRIGHT_BASE_URL=http://localhost:4321



# Node Environment
NODE_ENV=test
